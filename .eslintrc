{
  "root": true,
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": 0,
    "semi": 0,
    "comma-dangle": 0,
    "no-empty-function": "off",
    "max-len": "off",
    "no-useless-escape": "off",
    "react/prop-types": 0,
    "react/no-unknown-property": 0,
    "no-unused-vars": 0
  }
}
